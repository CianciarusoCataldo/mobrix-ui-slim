{
  "--mbx-rater-background": {
    "description": "Fallback value for `mbx-rater-background-light` and `mbx-rater-background-dark`"
  },
  "--mbx-rater-background-light": {
    "description": "Rater background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`",
    "defaultGlobal": "--mbx-background-light",
    "fallback": "--mbx-rater-background"
  },
  "--mbx-rater-background-dark": {
    "description": "Rater background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`",
    "defaultGlobal": "--mbx-background-dark",
    "fallback": "--mbx-rater-background"
  }
}
