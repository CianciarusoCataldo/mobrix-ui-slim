{
  "--mbx-codebox-background": {
    "description": "Fallback value for `mbx-codebox-background-light` and `mbx-codebox-background-dark`"
  },
  "--mbx-codebox-background-light": {
    "description": "CodeBox background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`",
    "defaultGlobal": "--mbx-background-light",
    "fallback": "--mbx-codebox-background"
  },
  "--mbx-codebox-background-dark": {
    "description": "CodeBox background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`",
    "defaultGlobal": "--mbx-background-dark",
    "fallback": "--mbx-codebox-background"
  },
  "--mbx-codebox-text-color": {
    "description": "Fallback value for `mbx-codebox-text-color-light` and `mbx-codebox-text-color-dark`",
    "isFallback": true
  },
  "--mbx-codebox-text-color-light": {
    "description": "CodeBox text-color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`",
    "defaultGlobal": "--mbx-text-color-light",
    "fallback": "--mbx-codebox-text-color"
  },
  "--mbx-codebox-text-color-dark": {
    "description": "CodeBox text-color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`",
    "defaultGlobal": "--mbx-text-color-dark",
    "fallback": "--mbx-codebox-text-color"
  }
}
